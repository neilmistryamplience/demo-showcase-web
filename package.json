{
  "name": "willow-demo-web",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "npm run build-sass && npm run build-ts && npm run copy-static-assets",
    "copy-static-assets": "node copyStaticAssets.js",
    "build-sass": "node-sass src/scss/main.scss dist/src/public/css/main.css",
    "watch-sass": "node-sass -w src/scss/main.scss dist/src/public/css/main.css",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "format": "prettier --write \"**/*.ts\"",
    "start": "npm run build && node $NODE_DEBUG_OPTION dist/src/main.js",
    "start:dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack $NODE_DEBUG_OPTION --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@types/aws-serverless-express": "^3.0.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/handlebars": "^4.0.39",
    "@types/source-map-support": "^0.4.1",
    "@types/uuid": "^3.4.3",
    "aws-serverless-express": "^3.3.5",
    "bootstrap": "^4.1.3",
    "cookie-parser": "^1.4.3",
    "dc-accelerators": "amplience/dc-accelerators#a420353",
    "express-handlebars": "^3.0.0",
    "express-hbs": "^1.0.4",
    "fastify-formbody": "^2.0.0",
    "fetch-ponyfill": "^6.0.2",
    "handlebars": "^4.0.11",
    "handlebars-helpers": "^0.10.0",
    "node-fetch": "^2.2.0",
    "node-sass": "^4.9.3",
    "node-sass-middleware": "^0.11.0",
    "r2": "^2.0.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "serverless-plugin-typescript": "^1.1.5",
    "showdown": "^1.8.6",
    "source-map-support": "^0.5.9",
    "typescript": "^2.6.2",
    "uuid": "^3.3.2",
    "webpack-dev-middleware": "latest"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "@types/webpack": "^4.4.10",
    "file-loader": "^1.1.11",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "serverless": "^1.30.1",
    "serverless-domain-manager": "^2.6.0",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^4.1.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "5.3.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
